{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["webworker", "es6", "scripthost"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
