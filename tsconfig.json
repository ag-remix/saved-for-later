{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["webworker", "es6", "scripthost"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
